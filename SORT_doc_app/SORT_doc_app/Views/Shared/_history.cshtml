
<table class="table">
    <tr>
        <th>
            Project
        </th>
        <th>
            Date
        </th>
        <th>
            Type
        </th>
        <th>
            Event
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.Events) {
    <tr>
        <td>
            @item.ProjectID
        </td>
        <td>
            @item._Date
        </td>
        <td>
            @item.EventType
        </td>
        <td>
            @item.EventBody
        </td>
    </tr>
}

</table>
